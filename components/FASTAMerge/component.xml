<component>
	<name>FASTAMerge</name>
	<version>1.0</version>
	<doc>Merge a list of FASTA files.</doc>
	<launcher type="python">
		<argument name="file" value="fastamerge.py" />
	</launcher>
	<requires URL="http://www.python.org/" type="manual">python</requires>
	<type-parameters>
		<type-parameter name="T"/>
		<type-parameter name="FASTA"/>
	</type-parameters>
	<inputs>
		<input name="fastafiles" type="T" array="true"></input>
	</inputs>
	<outputs>
		<output name="output" type="FASTA"></output>
	</outputs>
</component>
