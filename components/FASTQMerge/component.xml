<component>
	<name>FASTQMerge</name>
	<version>1.0</version>
	<doc>Merge a list of FASTQ files.</doc>
	<launcher type="python">
		<argument name="file" value="fastqmerge.py" />
	</launcher>
	<requires URL="http://www.python.org/" type="manual">python</requires>
	<type-parameters>
		<type-parameter name="T"/>
		<type-parameter name="FASTQ"/>
	</type-parameters>
	<inputs>
		<input name="in_array" type="T" array="true"></input>
	</inputs>
	<outputs>
		<output name="output" type="FASTQ"></output>
	</outputs>
</component>
