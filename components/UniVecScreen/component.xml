<component>
	<name>UniVecScreen</name>
	<version>1.0</version>
	<doc>Screen a FASTA file for vector sequences from the UniVec database.</doc>
	<launcher type="bash">
		<argument name="file" value="univecscreen.sh" />
	</launcher>
	<requires URL="http://www.gnu.org/software/bash/" type="manual">bash</requires>
	<type-parameters>
		<type-parameter name="FASTA"/>
		<type-parameter name="SSAHA2"/>
	</type-parameters>
	<inputs>
		<input name="fastafile" type="FASTA"></input>
	</inputs>
	<outputs>
		<output name="ssaha2out" type="SSAHA2"></output>
	</outputs>
	<parameters>
		<parameter name="rtype" type="string" default="abi"></parameter>
	</parameters>
</component>
