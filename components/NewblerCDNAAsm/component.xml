<component>
	<name>NewblerCDNAAsm</name>
	<version>1.0</version>
	<doc>Perform a transcriptome assembly using the newbler assembler.</doc>
	<launcher type="bash">
		<argument name="file" value="newblercdnaasm.sh" />
	</launcher>
	<requires URL="http://www.gnu.org/software/bash/" type="manual">bash</requires>
	<type-parameters>
		<type-parameter name="T"/>
		<type-parameter name="FASTA"/>
		<type-parameter name="NewblerASMDIR"/>
	</type-parameters>
	<inputs>
		<input name="array" type="T" array="generic"></input>
		<input name="vectortrimming" type="FASTA" optional="true"></input>
	</inputs>
	<outputs>
		<output name="assemblydir" type="NewblerASMDIR"></output>
	</outputs>
	<parameters>
		<parameter name="large" type="boolean" default="false"></parameter>
		<parameter name="cpu" type="int" default="1"></parameter>
	</parameters>
</component>
